<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/storage_settings_title"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="@string/storage_title_usb" android:key="usb_mode_category">
        <ListPreference android:entries="@array/usb_connection_mode_entries" android:title="@string/default_usb_mode" android:key="usb_mode" android:entryValues="@array/usb_connection_mode_values" />
    </PreferenceCategory>
    <PreferenceScreen android:title="@string/usb_connection_category" android:key="usb_select">
        <intent android:targetPackage="com.android.settings" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings.UsbSettings" />
    </PreferenceScreen>
</PreferenceScreen>